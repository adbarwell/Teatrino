(*# ErrorHandlingCrashBranch #*)

global protocol TravelAll(reliable role Cl, reliable role S, reliable role A) {
  rec t0 {
    choice at Cl {
      query(Int) from Cl to A;
      quote(Int) from A to Cl;
      dummy from A to S;
      continue t0;
    } or {
      ok from Cl to A;
      ok from A to S;
      payment(Int) from Cl to S;
      ack from S to Cl;

    } or {
      ko from Cl to A;
      ko from A to S;
      quit from Cl to A;

    }
  }
}
